    }

    .img {
        width: 80px;
        margin: auto;
        display: block;
    }

    .container {
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
        height: 100vh;
    }

    .child-container {
        width: 500px;
        padding: 15px;
        /* backdrop-filter: blur(20px); */
        border-radius: 32px;
        text-align: center;
    }

    .header-1 {
        font-weight: 700;
        font-size: 20px;
        text-align: center;
        letter-spacing: -0.02em;
        color: #667085;
    }

    .content {
        font-style: normal;
        font-weight: 400;
        font-size: 20px;
        line-height: 30px;
        text-align: center;
        color: #667085;
        width: 480px;
        display: block;
        margin: auto;
    }

    a {
        color: #43D9BB;
        text-decoration: none;
    }

    #footer {
        backdrop-filter: brightness(0.1);
        position: fixed;
        bottom: 0;
        width: 100%;
    }

    #footer p {
        font-style: normal;
        font-weight: 400;
        font-size: 16px;
        text-align: center;
        color: #98A2B3;
    }

    main {
      width: 100vw;
      display: flex;
      flex-direction: column;
      justify-content: center;
    }

    .table-area {
      width: 100%;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      max-width: 700px;
    }
    .table-area .wrapper {
      border-radius: 8px;
      border: 1px solid #000;
      background: #F0EEEE;
      padding: 6px;
    }
    .table-area table {
      width: 100%;
      border-radius: 8px;
      background: #F0EEEE;
      border-collapse: collapse;
    }
    .table-area thead {
      background-color: #000;
      border: 1px solid #000;
      color: #fff;
    }
    .table-area tbody {
      background-color: #fff;
    }
    .table-area th, td {
      padding: 12px 6px;
      text-align: center;
    }

    .content-wrapper {
      width: 100%;
      height: 100%;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      background: #FEEE5B;
      padding-top: 26px;
      padding-bottom: 26px;
    }

    @media screen and (max-width: 500px) {
        body {
            background-image: url('/images/app.bg.svg');
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            background-size: cover;
            position: relative;
            
            margin: 0;
            padding: 0;
            
        }

        .bg {
            width: 100%;
            height: 100%;
            bottom: 0;
            position: absolute;
            background: url('/images/not-found-background-mobile.png') no-repeat center bottom fixed;
                        -webkit-background-size: contain;
            -moz-background-size: contain;
            -o-background-size: contain;
            background-size: contain;
        }

        .content {
            width: 90%;
            padding-left: 1rem;
            padding-right: 1rem;
            font-size: 16px;
            line-height: 24px;
        }

        .container {
            margin-top: 10%;
            backdrop-filter: blur(20px);
        }

        .child-container {
            width: unset;
        }

        .header-1 {
          font-size: 16px;
        }

        #footer {
            backdrop-filter: brightness(0.1);
            position: fixed;
            bottom: 0;
            width: 100%;
            height: auto;
        }

        .content-wrapper {
          padding-left: 12px;
          padding-right: 12px;
        }

    }
  </style>
<body>
  <div class="container">
    <%- include('sidebar', { user: data.user }) %>
    <div class="content-wrapper">
      <div class="table-area">
        <!-- <div class="wrapper"> -->
          <table>
            <thead>
              <tr>
                <th>User Id</th>
                <th>Wallet address</th>
                <th>Wallet Balance</th>
                <!-- <th>usename</th> -->
                <th>Credit Balance</th>
                <th>Point Balance</th>

              </tr>
            </thead>
            <tbody>
              <% for(var i=0; i<data.wallets.length; i++) {%>
                <tr>
                  <td><%= data.wallets[i].userId %></td>
                  <td><%= data.wallets[i].walletAddress %></td>
                  <td><%= data.wallets[i].walletBalance %> gameUSD</td>
                  <td><%= data.wallets[i].creditBalance %> </td>
                  <td><%= data.wallets[i].pointBalance %> </td>
                </tr>
              <% } %>
            </tbody>
          </table>
      </div>

      <!-- <div class="pagination">
        <% if (data.currentPage > 1) { %>
          <a href="?page=<%= data.currentPage - 1 %>">Previous</a>
        <% } %>
      
        <% for (let i = 1; i <= data.totalPages; i++) { %>
